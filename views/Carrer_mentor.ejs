    <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Schedule a Demo</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <form class="row g-3" action="/send-email" method="POST">
                        <div class="col-md-6">
                            <label for="inputName" class="form-label">Name</label>
                            <input type="text" class="form-control" id="inputName" name="name" required>
                        </div>
                        <div class="col-md-6">
                            <label for="inputEmail4" class="form-label">Email</label>
                            <input type="email" class="form-control" id="inputEmail4" name="email" required>
                        </div>
                        <div class="col-6">
                            <label for="Mobile_Number" class="form-label">Mobile Number</label>
                            <input type="number" class="form-control" id="Mobile_Number" name="mobile" required>
                        </div>
                        <div class="col-md-6">
                            <label for="Current_Education_Level" class="form-label">Current Education Level</label>
                            <input type="text" class="form-control" id="Current_Education_Level" name="education" required>
                        </div>
                        <div class="col-md-6">
                            <label for="Language" class="form-label">Language</label>
                            <select id="Language" class="form-select" name="language">
                                <option selected>English</option>
                                <option>Hindi</option>
                                <option>Marathi</option>
                            </select>
                        </div>
                        <div class="col-md-6">
                            <label for="Career_Interests_Goals" class="form-label">Career Interests/Goals</label>
                            <input type="text" class="form-control" id="Career_Interests_Goals" name="careerGoals" required>
                        </div>
                        <div class="col-6">
                            <label for="appointment-date" class="form-label">Select a Date:</label>
                            <input type="date" id="appointment-date" class="form-control" name="date" required>
                        </div>
                        <div class="col-6">
                            <label for="appointment-time" class="form-label">Select a Time:</label>
                            <input type="time" id="appointment-time" class="form-control" name="time" required>
                        </div>
                        <div class="col-md-4">
                            <button type="submit" class="btn btn-primary">Schedule a Demo</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
